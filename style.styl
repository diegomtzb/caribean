  .nav-pills > li {
    float: left;
  }


  .nav-pills > li > a
    text-decoration: none;
    width: 11em


  .nav-pills > li > a:after {
    content: '';
    display: block;
    border-bottom: 1px solid white;
    width: 0;
    -webkit-transition: 0.3s ease;
    transition: 0.3s ease;
  }

  //Ojo funcionalidad de subrayado
